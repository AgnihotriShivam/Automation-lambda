{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "iam:GetRole",
                "iam:PassRole"
            ],
            "Resource": "arn:aws:iam::*:role/TSI_AWS_configRole"
        },
        {
            "Effect": "Allow",
            "Action": [
                "config:*"
            ],
            "Resource": "*"
        }
    ]
}