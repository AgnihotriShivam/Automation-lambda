{
    "pipeline": {
        "name": "Virago-Development",
        "roleArn": "arn:aws:iam::412318185247:role/PIPELINEDEV_eu-central-1_ServiceRoleForCodePipeline",
        "artifactStore": {
            "type": "S3",
            "location": "412318185247-pipeline"
        },
        "stages": [
            {
                "name": "Source",
                "actions": [
                    {
                        "name": "Source",
                        "actionTypeId": {
                            "category": "Source",
                            "owner": "AWS",
                            "provider": "S3",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "PollForSourceChanges": "false",
                            "S3Bucket": "git-to-amazon-s3-outputbucket-1fuwlpd9otnff",
                            "S3ObjectKey": "virago/ViragoProject/virago_ViragoProject.zip"
                        },
                        "outputArtifacts": [
                            {
                                "name": "viragobuildsource"
                            }
                        ],
                        "inputArtifacts": []
                    }
                ]
            },
            {
                "name": "Build",
                "actions": [
                    {
                        "name": "Codebuild",
                        "actionTypeId": {
                            "category": "Build",
                            "owner": "AWS",
                            "provider": "CodeBuild",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "ProjectName": "LambdaDeployer"
                        },
                        "outputArtifacts": [
                            {
                                "name": "viragobuildsourceoutput"
                            }
                        ],
                        "inputArtifacts": [
                            {
                                "name": "viragobuildsource"
                            }
                        ]
                    }
                ]
            },
            {
                "name": "Test-Provisioning",
                "actions": [
                    {
                        "name": "AcceptanceTest",
                        "actionTypeId": {
                            "category": "Invoke",
                            "owner": "AWS",
                            "provider": "Lambda",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "FunctionName": "mockPipeline-CodePipelineLambdaDummy",
                            "UserParameters": "mockPipeline"
                        },
                        "outputArtifacts": [],
                        "inputArtifacts": []
                    }
                ]
            },
            {
                "name": "Test-Baseline",
                "actions": [
                    {
                        "name": "AcceptanceTest",
                        "actionTypeId": {
                            "category": "Invoke",
                            "owner": "AWS",
                            "provider": "Lambda",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "FunctionName": "mockPipeline-CodePipelineLambdaDummy",
                            "UserParameters": "mockPipeline"
                        },
                        "outputArtifacts": [],
                        "inputArtifacts": []
                    }
                ]
            },
            {
                "name": "Production",
                "actions": [
                    {
                        "name": "Approval",
                        "actionTypeId": {
                            "category": "Approval",
                            "owner": "AWS",
                            "provider": "Manual",
                            "version": "1"
                        },
                        "runOrder": 1,
                        "configuration": {
                            "CustomData": "The latest changes include feedback from Bob.",
                            "ExternalEntityLink": "http://example.com"
                        },
                        "outputArtifacts": [],
                        "inputArtifacts": []
                    },
                    {
                        "name": "SwitchEnvironments",
                        "actionTypeId": {
                            "category": "Invoke",
                            "owner": "AWS",
                            "provider": "Lambda",
                            "version": "1"
                        },
                        "runOrder": 2,
                        "configuration": {
                            "FunctionName": "mockPipeline-CodePipelineLambdaDummy",
                            "UserParameters": "mockPipeline"
                        },
                        "outputArtifacts": [],
                        "inputArtifacts": []
                    }
                ]
            }
        ],
        "version": 4
    }
}
